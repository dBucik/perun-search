    getRichUsersByName(titleBefore: String, firstName: String, middleName: String, lastName: String, titleAfter: String): [RichUser!]!
    getRichUsersByServiceAcc(isServiceAcc: Boolean): [RichUser!]!
    getRichUsersBySponsoredAcc(isSponsoredAcc: Boolean): [RichUser!]!
