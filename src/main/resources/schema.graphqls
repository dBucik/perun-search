    # FACILITY
    getFacility(id: Long!): Facility
    getFacilities: [Facility!]!
    getFacilitiesByName(name: String!): [Facility!]!
    getFacilitiesHavingAttrs(attrs: [InputAttribute!]!): [Facility!]!

