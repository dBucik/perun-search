    getMember(id: Long!): Member
    getMembers: [Member!]!
    getMembersOfUser(userId: Long!): [Member!]!
    getMembersOfVo(voId: Long!): [Member!]!
    getMembersByStatus(status: String!): [Member!]!
    getMembersBySponsored(isSponsored: Boolean!): [Member!]!
    getMembersHavingAttrs(attrs: [InputAttribute!]!): [Member!]!

    # RICH_MEMBER
    getRichMember(id: Long!): RichMember
    getRichMembers: [RichMember!]!
    getRichMembersOfUser(userId: Long!): [RichMember!]!
    getRichMembersOfVo(voId: Long!): [RichMember!]!
    getRichMembersByStatus(status: String!): [RichMember!]!
    getRichMembersBySponsored(isSponsored: Boolean!): [RichMember!]!
    getRichMembersHavingAttrs(attrs: [InputAttribute!]!): [RichMember!]!

