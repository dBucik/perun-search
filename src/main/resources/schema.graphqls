    getExtSource(id: Long!): ExtSource
    getExtSources: [ExtSource!]!
    getExtSourcesByName(name: String!): [ExtSource!]!
    getExtSourcesByType(type: String!): [ExtSource!]!
    getExtSourcesHavingAttrs(attrs: [InputAttribute!]!): [ExtSource!]!

    # RICH_EXT_SOURCE
    getRichExtSource(id: Long!): RichExtSource
    getRichExtSources: [RichExtSource!]!
